<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
  <link rel="stylesheet" type="text/css" href="/public/css/common.css">
  <script type='text/javascript' src='/public/js/block.js'></script>

</head>

<body>

  <%- include('partials/header',{user:userId}) -%>
<div class="container reg_form" style="margin:10px; margin-left:100px;background:#111">
	<h4 class="text-center">Company Details</h4>
<form class="form-horizontal" role="form">
<div class="container">
	<div class="row clearfix">
		<div class="col-md-4 col-md-push-1">
			<table class="table table-bordered" id="tab_logic_total">
				<tbody>
					<tr>
						<th class="text-center">Registration Date</th>
						<td class="text-center">
								<input type="text" value="<%= data.registration_date %>" class="form-control" readonly/>
							</td>
					</tr>
					<tr>
						<th class="text-center">Company Name </th>
						<td class="text-center"><input type="text" value="<%= data.company_name %>" class="form-control" id="cin_no" readonly /></td>
					</tr>
					<tr>

						<th class="text-center">Company GSTIN</th>
						<td class="text-center"><input type="text"  value="<%= data.gstin %>" class="form-control" id="gst_no" readonly /></td>
					</tr>
				</tbody>
				</table>
					</div>
					<div class="col-md-5 col-md-push-1">
						<table class="table table-bordered" id="tab_logic_total">
						<tbody>
							<tr>
						<th class="text-center">Company CIN</th>
						<td class="text-center"><input type="text" id="cin" value="<%= data.company_id %>"class="form-control" readonly/></td>
					</tr>
					<tr>
						<th class="text-center">Company Email</th>
						<td class="text-center"><input type="text" value="<%= data.email %>" class="form-control" readonly/></td>
					</tr>
					<tr>
						<th class="text-center">Company Contact</th>
						<td class="text-center"><input type="number" value="<%= data.contact_no %>"  class="form-control" readonly/></td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
	</div>
<div class="container">
  <div class="row clearfix">
    <div class="col-md-10">
			<hr>
      <table class="table table-bordered" id="tab_logic">
        <thead>
          <tr>
						<th class="text-center">#</th>
            <th class="text-center"> Bank Name </th>
            <th class="text-center"> Account Number </th>
            <th class="text-center"> Branch </th>
            <th class="text-center"> Account Holder </th>
          </tr>
        </thead>
        <tbody>
          <tr id='addr0'>
						<td>1</td>
            <td><input type="text" value="<%= data.bank_name %>" class="form-control" readonly/></td>
            <td><input type="text" value="<%= data.account_no %>" class="form-control" readonly/></td>
            <td><input type="text" value="<%= data.branch %>" class="form-control" readonly/></td>
            <td><input type="text" value="<%= data.account_holder_name %>" class="form-control" readonly/></td>
          </tr>
        </tbody>
      </table>
			<hr>
    </div>
  </div>

  <div class="row clearfix" style="margin-top:20px">
    <div class="col-lg-10">

      <table class="table table-bordered" id="tab_logic_total">
        <tbody>
          <tr>
            <th class="text-center">Primary Key</th>
            <td class="text-center"><input type="text" value="<%= data.private_key %>"class="form-control" readonly/></td>
          </tr>
          <tr>
            <th class="text-center">Public Key</th>
            <td class="text-center"><input type="text" value="<%= data.public_key %>"class="form-control" readonly/></td>
          </tr>
          <tr>
            <th class="text-center">Mod_N</th>
            <td class="text-center"><input type="text" value="<%= data.mod_n %>"class="form-control" readonly/></td>
          </tr>
          <tr>
            <th class="text-center">Account Flag</th>
            <td class="text-center"><input type="text" value="<%= data.active_flag %>"class="form-control" readonly/></td>
          </tr>
        </tbody>
				<hr>
      </table>

    </div>
  </div>
</div>
<div class="row clearfix">
	<center>
    <% if (data.active_flag=="Active") { %>
		<button   id="block" class="btn btn-primary">Block User</button>
    <% } else { %>
     	<button  id="Unblock" class="btn btn-primary">Unblock User</button>
    <% } %>
	</center>
</div>
</br>
</br>
</form>
</div>
<%- include('partials/footer.ejs') -%>
</body>
<html>
